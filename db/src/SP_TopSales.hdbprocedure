PROCEDURE "APP26012022_DB_SP_TopSales" (IN amount INTEGER, OUT result "APP26012022_DB_TT_TopSales")
  LANGUAGE SQLSCRIPT
  SQL SECURITY INVOKER
  READS SQL DATA
  AS
BEGIN
  result = select ID, REGION as "region", COUNTRY as "country", AMOUNT as "amount", COMMENTS as "comments" 
             from "APP26012022_DB_SALES"
             where AMOUNT >= :amount
             order by AMOUNT desc
             ;
END